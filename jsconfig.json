{
    "compilerOptions": {
        "target": "es2017",
        "allowSyntheticDefaultImports": false,
        "baseUrl": "./",
        "paths": {
            "Config/*": [
                "src/config/*"
            ],
            "@presentational/*": [
                "src/components/presentational/*"
            ],
            "@container/*": [
                "src/components/container/*"
            ],
            "Shared/*": [
                "src/shared/*"
            ],
            "App/*": [
                "src/*"
            ]
        }
    },
    "exclude": [
        "node_modules",
        "dist"
    ]
}